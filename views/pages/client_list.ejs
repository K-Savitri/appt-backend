

<div class="content-header">
    <p class="todays-header">Client List</p>
</div>

<div class="table-responsive">
    <table class="table table-bordered table-striped table-hover" id="clientTable">
        <thead class="table-dark">
            <tr>
                <th>Customer Name</th>
                <th>Phone Number</th>
                <th>Email ID</th>
                <th>No. of Visits</th>
                <th>Last Visited</th>
            </tr>
        </thead>
        <tbody>
            <% clients.forEach(client => { %>
                <tr>
                    <td><%= client.c_first_name %> <%= client.c_last_name %></td>
                    <td><%= client.c_phone %></td>
                    <td><%= client.c_email %></td>
                    <td><%= client.visits %></td>
                    <td><%= client.lastVisited %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>
